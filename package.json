{
  "name": "easypub_app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --modern --mode production",
    "lint": "vue-cli-service lint",
    "test:e2e": "vue-cli-service test:e2e",
    "test:unit": "vue-cli-service test:unit",
    "format": "prettier-eslint \"src/**/*.*(vue|ts|js|gql)\" --write"
  },
  "dependencies": {
    "@absinthe/socket": "^0.1.10",
    "@absinthe/socket-apollo-link": "^0.1.11",
    "apollo-absinthe-upload-link": "^1.2.2",
    "apollo-cache-inmemory": "^1.3.8",
    "apollo-client": "^2.4.5",
    "apollo-link": "^1.2.3",
    "apollo-link-context": "^1.0.9",
    "apollo-utilities": "^1.0.25",
    "localforage": "^1.7.2",
    "phoenix": "^1.3.4",
    "ramda": "^0.25.0",
    "register-service-worker": "^1.0.0",
    "vee-validate": "^2.1.0-beta.9",
    "vue": "^2.5.17",
    "vue-analytics": "^5.16.0",
    "vue-apollo": "^3.0.0-beta.11",
    "vue-qrcode-reader": "^1.2.2",
    "vue-router": "^3.0.1",
    "vuetify": "^1.2.0",
    "vuex": "^3.0.1",
    "vuex-persist": "^1.7.2",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@types/graphql": "^14.0.3",
    "@types/jest": "^23.1.4",
    "@types/jwt-decode": "^2.2.1",
    "@types/ramda": "types/npm-ramda#dist",
    "@vue/cli-plugin-babel": "^3.0.4",
    "@vue/cli-plugin-e2e-cypress": "^3.0.4",
    "@vue/cli-plugin-eslint": "^3.0.4",
    "@vue/cli-plugin-pwa": "^3.0.4",
    "@vue/cli-plugin-typescript": "^3.1.0",
    "@vue/cli-plugin-unit-jest": "^3.0.4",
    "@vue/cli-service": "^3.0.4",
    "@vue/eslint-config-standard": "^3.0.4",
    "@vue/eslint-config-typescript": "^3.0.5",
    "@vue/test-utils": "^1.0.0-beta.20",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^23.0.1",
    "babel-loader": "^8.0.4",
    "graphql-tag": "^2.9.0",
    "lint-staged": "^7.2.2",
    "prettier-eslint-cli": "^4.7.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2",
    "ts-jest": "^23.0.0",
    "typescript": "^3.0.0",
    "vue-cli-plugin-apollo": "^0.17.0",
    "vue-cli-plugin-vuetify": "^0.2.0",
    "vue-template-compiler": "^2.5.17"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "prettier-eslint \"src/**/*.js\" --write",
      "vue-cli-service lint",
      "git add"
    ],
    "*.ts": [
      "prettier-eslint \"src/**/*.ts\" --write",
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "prettier-eslint \"src/**/*.vue\" --write",
      "vue-cli-service lint",
      "git add"
    ],
    "*.gql": [
      "prettier-eslint \"src/**/*.gql\" --write",
      "git add"
    ]
  }
}
